@model SanCamp.Domain.Models.QuerySearchResultModel
@{
    ViewData["Title"] = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@if (Model != null && Model.Cities.Count > 0)
{
    <h1>View</h1>

    <table class="table">
        <thead>
            <tr>
                <th>
                    CityName
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.Cities)
            {
                <tr>
                    <td>
                        @item.Name
                    </td>
                </tr>
            }
        </tbody>
    </table>
}

